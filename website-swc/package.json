    "@ant-design/icons": "^4.4.0",
    "antd": "^4.11.2",
    "axios": "^1.1.3",
    "babel-loader": "^8.2.3",
    "copy-to-clipboard": "^3.3.1",
    "gh-pages": "^3.1.0",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "vite": "^4.3.9"
